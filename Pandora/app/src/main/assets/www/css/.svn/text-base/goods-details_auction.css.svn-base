}

/*竞拍记录开始*/
.auction_status{
	height: 35px;
	line-height:35px;
	align-items:center;
	padding:0px 10px;
	display:none;
}
.auction_status_doing{
	background-color:#E7A840;
	color:#fff;
}
.auction_status_accounting{
	background-color:#DDDDDD;
	color:#FEFEFE;
}
.auction_status_done{
	background-color:#DDDDDD;
	color:#FEFEFE;
}
.auction_result{
	display:none;
}

.auction_button{
	display:none;
}
.auction_success{
	display:none;
}
.auction_success_icon{
	height:35px;
}

.auction_title{
	height:45px;
	line-height:45px;
	align-items:center;
	display:-webkit-flex;
	padding:0 10px;
	background-color:#fff;
	border-bottom:1px solid #EEEEEE;
}
.auction_title_name{
	float:left;
	width:70%;
	font-size:14px;
	color:#666666;
}
.auction_title_all{
	text-align:right;
	font-size:12px;
	color:#999999;
	width:30%;
}
.auction_title_all img{
    margin-left:5px;
	height:12px;
	display: inline-block; 
	position:relative;
	top:2px;
}
.ar_box{
	width: 100%;
	background: #FFFFFF;
}
.ar_box li{
	height:55px;
	align-items:center;
	margin: 0 10px;
	position: relative;
	padding-top:15px;
}
.ar_box li + li{
	border-top:1px solid #EEEEEE;
}
.ar_box li:first-child .ar_money{
	color:#FE7F7F;
}
.ar_img{
	width:15%;
	float: left;
}
.arImg{
	/*display: inline-block;*/
	display:table-cell;
	vertical-align: middle;

	
}
.arImg img{
	width:40px;
	height:40px;
	border:1px solid #81D8D0;
	border-radius:50%;
}
.ar_cont{
	width:73%;
	padding-left:5px;
	float: left;	
}
.ar_cont span{
	display: block;
	line-height: none !important;
}
.arCont_title{
font-size:16px;
margin-bottom:5px;
white-space: nowrap;
overflow: hidden; 
text-overflow: ellipsis;
display:inline-block;
-webkit-line-clamp:1;
-webkit-box-orient: vertical;	
}
.arCont_time{
	font-size:14px;
	color:#bdbdbd;
}
.ar_money{
	width:30%;
	float: right;
	font-size: 14px;
	color: #999;
	position: absolute;
	top:24px;
	right:10px;
	text-align:right;
}
.ar_money i{
	font-size: 16px;
}
.colorRed{
	color:#ff6565;
	
}
/*竞拍记录结束*/