@charset "utf-8";
/*公共样式*/
html{font-size:62.5%;font-family:Helvetica;tap-highlight-color:rgba(255,255,255,0);-webkit-tap-highlight-color:rgba(255,255,255,0);}body,div,header,footer,ul,ol,li,h1,h2,h3,h4,h5,h6,input,button,p,table,th,td,em,strong{padding:0;margin:0;border:none;font-style:normal;font-family:'STHeitiSC-Medium';font-weight:normal;}ul,li{list-style:none;}input,button{-webkit-appearance:none;border:none;outline:none}a{color:#000;text-decoration:none;-webkit-touch-callout:none;}a:visited,a:hover{text-decoration:none;}i,em{font-style:normal;}
.clear{clear:both;}
/*去除scroll滚动条样式*/
::-webkit-scrollbar{width:0px;height:14px}
::-webkit-scrollbar-button:vertical:increment{background:#fff;}
::-webkit-scrollbar-button:horizontal:increment{background:#fff url(/FronWeb/Images/y1204.png) no-repeat 2px 0;width:14px}
::-webkit-scrollbar-button:vertical:decrement{background:#fff url(/FronWeb/Images/s1204.png) no-repeat 1px -2px;height:14px}
::-webkit-scrollbar-button:horizontal:decrement{background:#fff url(/FronWeb/Images/z1204.png) no-repeat 1px 0;width:14px}
::-webkit-scrollbar-track-piece{background-color:#fff}
::-webkit-scrollbar-thumb:vertical{height:50px;background-color:#fff;border:1px solid #fff;border-radius:10px}
::-webkit-scrollbar-thumb:horizontal{height:50px;background-color:#fff;border:1px solid #fff;border-radius:10px}
/*弹出框的样式*/

.box{position:fixed;width:100%;height:100%;background:rgba(0,0,0,.7);display:none;top:0;left:0;z-index:9999;}
.dialog-bd{border-radius:6px;-webkit-background-clip:padding-box;background-clip:padding-box;outline:0;pointer-events:auto;background-color:rgba(253,253,253,.95);position:relative;width:80%;position:fixed;left:10%;top:25%;background:#fff}
.dialog-more{min-height:71px;border-top-left-radius:6px;border-top-right-radius:6px;padding:18px;font-size:1.6rem;display:-webkit-box;display:box;-webkit-box-pack:center;-webkit-box-align:center;-webkit-box-orient:vertical;color:#6D6D6D;}
.ui-dialog-ft{border-bottom-left-radius:6px;border-bottom-right-radius:6px}
.ui-dialog-ft a{height:42px;line-height:42px;background:0 0}
.ui-dialog-ft a:active{background:rgba(0,0,0,.1)}
.ui-dialog-ft a:first-child{border-bottom-left-radius:6px;background-image:none}
.ui-dialog .ui-dialog-ft a:last-child{border-bottom-right-radius:6px}
.ui-btn-group{display:-webkit-box;width:100%}
.ui-btn-group a{color:#6D6D6D;border:none;display:block;line-height:50px;height:48px;-webkit-box-flex:1;text-align:center;font-size:1.3rem;border-right:1px #c8c7cc solid;width:100%;border-top:1px solid #c8c7cc;text-decoration:none;}
.ui-btn-group a:active{background:rgba(0,0,0,.1)}
.ui-btn-group a:last-child{border-right:none;background-image:none;background:#81D8D0;color:#fff;border-bottom-right-radius:6px}
@media screen and (-webkit-min-device-pixel-ratio:2){.ui-btn-group button{position:relative;border:none}
.ui-btn-group a:before{content:"";position:absolute;-webkit-transform-origin:right top;width:0;border-right:1px solid #c8c7cc;right:0;top:0;bottom:0;-webkit-transform:scaleX(.5)}
.ui-btn-group a:after{content:"";position:absolute;-webkit-transform-origin:right top;left:0;right:0;height:0;border-top:1px solid #c8c7cc;top:0;-webkit-transform:scaleY(.5)}
.ui-btn-group a:last-child:before{border-right:none;background-image:none}
}

/*头部header*/
.ui-page-navbar {
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    font-size: 17px;
    position: relative;
    margin: 0;
    z-index: 9999;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
}
.ui-page-inner {
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 44px;
    height:44px;
}
.ui-page-navbar {
    _position: absolute;
    left: 0;
    top: 0;
background: #81D8D0;                                                       
}

.ui-navbar-through {}
.ui-navbar-left {
    position: absolute;
    left: 0px;
}
.ui-navbar-text {
    color:#ffffff;
    font-size: 17px;
    font-weight: 500;
    width: auto;
    margin: 0;
    overflow: hidden;
    text-overflow: ellipsis;
}
.ui-navbar-right {
    position: absolute;
    right: 0px;
}
.ui-operate {width: auto; padding-left: 10px; padding-right: 20px;}
.ui-operate-icon {width: 44px;overflow: hidden;}
.ui-operate a,.ui-operate-icon a {
    line-height: 44px;
    height: 44px;
    text-decoration: none;
    position: relative;
    color:#ffffff;
}
i.icon {
  display: inline-block;
  vertical-align: middle;
  background-size: 100% auto;
  background-position: center;
  background-repeat: no-repeat;
  font-style: normal;
  position: relative;
  top:-2px;
}
i.icon.icon-back {
  width: 12px;
  height: 20px;
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 12 20'><path d='M10,0l2,2l-8,8l8,8l-2,2L0,10L10,0z' fill='%23ffffff'/></svg>");
  }
  /*头部end*/
 

/*加载更多*/
#wrapper {
	position: absolute;
	z-index: 1;
	width: 100%;
}
.scroller {
	position: absolute;
	z-index: 1;
	/*	-webkit-touch-callout:none;*/
	-webkit-tap-highlight-color: rgba(0,0,0,0);
	width: 100%;
	padding: 0;
}
.scroller ul {
	list-style: none;
	padding: 0;
	margin: 0;
	width: 100%;
	text-align: left;
}
.scroller li {
	min-height:70px;
	background-color: #fafafa;
	font-size: 14px;
	padding:5px;
}
.pullDown, .pullUp {
	text-align: center;
	height: 40px;
	line-height: 40px;
	font-size: 12px;
	color: #888;
	font-family: Arial, Microsoft YaHei;
} .pullUp{ display:block;}
.loader {
	display: inline-block;
	font-size: 0px;
	padding: 0px;
	display: none;
}
.loader span {
	vertical-align: middle;
	border-radius: 100%;
	display: inline-block;
	width: 10px;
	height: 10px;
	margin: 0 2px;
	-webkit-animation: loader 0.8s linear infinite alternate;
	animation: loader 0.8s linear infinite alternate;
}
.loader span:nth-child(1) {
	-webkit-animation-delay: -1s;
	animation-delay: -1s;
	background: rgba(245, 103, 115,0.6);
}
.loader span:nth-child(2) {
	-webkit-animation-delay: -0.8s;
	animation-delay: -0.8s;
	background: rgba(245, 103, 115,0.8);
}
.loader span:nth-child(3) {
	-webkit-animation-delay: -0.26666s;
	animation-delay: -0.26666s;
	background: rgba(245, 103, 115,1);
}
.loader span:nth-child(4) {
	-webkit-animation-delay: -0.8s;
	animation-delay: -0.8s;
	background: rgba(245, 103, 115,0.8);
}
.loader span:nth-child(5) {
	-webkit-animation-delay: -1s;
	animation-delay: -1s;
	background: rgba(245, 103, 115,0.4);
}
 @keyframes loader {
 from {
transform: scale(0, 0);
}
to {
	transform: scale(1, 1);
}
}
@-webkit-keyframes loader {
 from {
-webkit-transform: scale(0, 0);
}
to {
	-webkit-transform: scale(1, 1);
}
}

