.tabs {
	height: 40px;
	border-bottom: 1px solid #EEEEEE;
}

.tabs a {
	display: block;
	float: left;
	width: 50%;
	color: #949494;
	text-align: center;
	background: #FFFFFF;
	line-height: 40px;
	font-size: 1.6rem;
	text-decoration: none;
}
.tabs a.active {
	color: #A1E1DB;
	background: #FFFFFF;
	/*border-radius: 5px 5px 0px 0px;*/
}
.swiper-container {
	background: none;
	border-radius: 0 0 5px 5px;
	width: 100%;
	border-top: 0;
}
.swiper-slide {
	width: 100%;
	background: none;
}
.content-slide {
/*	padding: 40px;*/
}
.content-slide p {
	text-indent: 2em;
	line-height: 1.9;
	text-align: center;
}
.content-slide ul{
	/*padding: 1rem;*/
	width: 100%;
}
.content-slide ul li{
	float: left;
	width: 50%;
	display: inline-block;
  width: -webkit-calc( (100% -1px*1) / 2);
  width: calc( (100% - 1px*1) / 2);
  border-bottom: 1px solid #EEEEEE;
  border-right: 1px solid #EEEEEE;
  height: 200px;
  text-align: center;
  vertical-align: middle;
  position: relative;
}
.content-slide ul li:nth-child(2n){
	  border-right:none;
}
.content-slide ul li h4{
	padding: 1rem 0;
	font-size: 1.4rem;
}
.content-slide ul li h4 span{
	padding: 1rem 0;
		color: #A1E1DB;
}
.content-slide ul li h4 i{
	color: #333333;
}
.content-slide ul li h4 em{
	color: #999999;
}
.content-slide_img{
	display: block;
	text-align: center;
	height: 80px;
}
.content-slide_img img{
	width: 80px;
	height: 80px;
	border: 1px solid #F5F5F5;
	margin: 0 auto;
	border: 0;
/*	position: relative;*/
}
