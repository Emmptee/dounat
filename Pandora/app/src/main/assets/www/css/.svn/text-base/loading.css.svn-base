body{
	-moz-user-select:none;/*火狐*/
	-webkit-user-select:none;/*webkit浏览器*/
	-ms-user-select:none;/*IE10*/
	-khtml-user-select:none;/*早期浏览器*/
	user-select:none;
}

.infopage {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 62.5%;

    width: 100%;
    height: 100%;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-pack: center;
    -webkit-box-align: center;
    text-align: center;
    position: absolute;
    top: 0px;
    z-index: 999;
    background: white;
}
.ui-notice,.no_wifi,.nothing {
    width: 100%;
    height: 100%;
    z-index: 999;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-pack: center;
    -webkit-box-align: center;
    position: absolute;
    text-align: center;
}

/*display: block;*/
.ui-notice i img{

    width: 37px;
    margin-bottom: 20px;
}
.ui-notice p,.no_wifi p,.nothing p{
    font-size: 14px;
    line-height: 20px;
    color: #999999;
    text-align: center;
    padding: 0 15px;
}
.no_wifi i img,.nothing i img{
    display: block;
    width: 37px;
    margin-bottom: 20px;
}