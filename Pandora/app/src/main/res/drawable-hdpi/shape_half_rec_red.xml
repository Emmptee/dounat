<?xml version="1.0" encoding="utf-8"?><!--空心圆角矩形  -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">

    <corners android:radius="5dp"/>
    <!-- 圆角半径 -->

    <stroke
        android:width="1dp"
        android:color="#ff6565"/>
    <!--矩形的边线  -->

    <gradient
        android:endColor="#ff6565"
        android:startColor="#ff6565"/>
    <!-- 矩形的内部颜色，这里设置成白色，即空心 -->

</shape>
