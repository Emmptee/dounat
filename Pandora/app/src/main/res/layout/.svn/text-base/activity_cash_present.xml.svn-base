<?xml version="1.0" encoding="utf-8"?>
<com.bis.android.plug.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical"
    tools:context="com.donut.app.activity.CashPresentActivity">

    <include layout="@layout/head_include" />

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_100"
        android:paddingLeft="@dimen/px_20"
        android:paddingTop="@dimen/px_20">

        <TextView
            android:id="@+id/cash_present_tv_startTime"
            android:layout_width="@dimen/px_210"
            android:layout_height="@dimen/px_60"
            android:background="@drawable/shape_btn_gray"
            android:gravity="center"
            android:text="----/--/--"
            android:textColor="@color/text_gray3"
            android:textSize="@dimen/text_second_font_size" />

        <TextView
            android:layout_width="@dimen/px_80"
            android:layout_height="@dimen/px_60"
            android:gravity="center"
            android:text="åˆ°"
            android:textColor="@color/text_gray3"
            android:textSize="@dimen/text_second_font_size" />

        <TextView
            android:id="@+id/cash_present_tv_endTime"
            android:layout_width="@dimen/px_210"
            android:layout_height="@dimen/px_60"
            android:background="@drawable/shape_btn_gray"
            android:gravity="center"
            android:text="----/--/--"
            android:textColor="@color/text_gray3"
            android:textSize="@dimen/text_second_font_size" />

        <TextView
            android:id="@+id/cash_present_tv_search"
            android:layout_width="@dimen/px_180"
            android:layout_height="@dimen/px_60"
            android:layout_marginLeft="@dimen/px_30"
            android:background="@drawable/shape_btn_main"
            android:gravity="center"
            android:text="@string/search"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_second_font_size" />

    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_20"
        android:background="@color/gray_lighter_f2" />

    <com.bis.android.plug.autolayout.AutoFrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/cash_present_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none" />

        <TextView
            android:id="@+id/cash_present_tv_msg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/px_180"
            android:drawablePadding="15dp"
            android:drawableTop="@drawable/no_msg_icon"
            android:text="@string/cash_present_no_mag"
            android:textColor="@color/text_gray9"
            android:textSize="@dimen/text_second_font_size"
            android:visibility="gone" />

    </com.bis.android.plug.autolayout.AutoFrameLayout>
</com.bis.android.plug.autolayout.AutoLinearLayout>
