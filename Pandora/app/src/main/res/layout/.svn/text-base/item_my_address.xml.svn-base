<?xml version="1.0" encoding="utf-8"?>
<com.bis.android.plug.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/white">

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/px_20"
        android:paddingRight="@dimen/px_20"
        android:paddingTop="@dimen/px_35"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/my_nickname"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/text_gray3"
            android:textSize="28px"
            android:text="收货人：某某"/>
        <TextView
            android:id="@+id/my_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_gray3"
            android:textSize="28px"
            android:text="15910618467"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>
    <TextView
        android:id="@+id/my_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/text_gray3"
        android:textSize="28px"
        android:paddingLeft="@dimen/px_20"
        android:paddingRight="@dimen/px_20"
        android:layout_marginTop="@dimen/px_20"
        android:layout_marginBottom="@dimen/px_35"
        android:lineSpacingExtra="@dimen/px_5"
        android:text="收货地址：海淀区中关村国际软件大厦新思软件科技有限公司"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/line_gray2"/>

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="88px"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/px_20"
        android:paddingRight="@dimen/px_20"
        android:orientation="horizontal">
      <CheckBox
          android:id="@+id/addr_default"
          android:layout_width="0dp"
          android:layout_height="match_parent"
          android:layout_weight="1"
          android:button="@null"
          android:background="@null"
          android:drawableLeft="@drawable/address_selector"
          android:drawablePadding="@dimen/px_10"
          android:textSize="24px"
          android:checked="true"
          android:textColor="@color/text_gray3"
          android:text="@string/default_addr_set"/>
        <TextView
            android:id="@+id/addr_edit"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:drawableLeft="@drawable/icon_addr_edit"
            android:drawablePadding="@dimen/px_10"
            android:textSize="24px"
            android:textColor="@color/text_gray3"
            android:text="@string/edit"/>
        <TextView
            android:id="@+id/addr_del"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:drawableLeft="@drawable/icon_addr_del"
            android:layout_marginLeft="@dimen/px_80"
            android:drawablePadding="@dimen/px_10"
            android:textSize="24px"
            android:textColor="@color/text_gray3"
            android:text="@string/delete"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_20"
        android:background="@color/gray_lighter_f2"/>

</com.bis.android.plug.autolayout.AutoLinearLayout>