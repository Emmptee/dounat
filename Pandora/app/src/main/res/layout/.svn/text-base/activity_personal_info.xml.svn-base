<?xml version="1.0" encoding="utf-8"?>
<com.bis.android.plug.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include
        layout="@layout/head_include"/>

    <TextView
        android:id="@+id/vip_tips"
        android:layout_width="match_parent"
        android:layout_height="88px"
        android:background="#e2faf8"
        android:gravity="center"
        android:textColor="@color/colorPrimary"
        android:textSize="28px"/>

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:id="@+id/header_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/px_20">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/head"
            android:textColor="@color/text_gray3"
            android:textSize="30px"/>

        <ImageView
            android:id="@+id/per_header"
            android:layout_width="66dp"
            android:layout_height="66dp"
            android:background="@drawable/shape_ring_round_main"
            android:padding="1dp"
            android:src="@drawable/default_header"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/px_20"
        android:background="@color/bg_img_gray"/>

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:id="@+id/nickname_layout"
        android:layout_width="match_parent"
        android:layout_height="88px"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/px_20">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/nickname"
            android:textColor="@color/text_gray3"
            android:textSize="30px"/>

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="@dimen/px_20"
            android:drawableRight="@drawable/icon_next"
            android:text="@string/no_setting"
            android:textColor="@color/text_gray3"
            android:textSize="28px"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/px_20"
        android:background="@color/bg_img_gray"/>

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:id="@+id/sex_layout"
        android:layout_width="match_parent"
        android:layout_height="88px"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/px_20">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/sex"
            android:textColor="@color/text_gray3"
            android:textSize="30px"/>

        <TextView
            android:id="@+id/tv_sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="@dimen/px_20"
            android:drawableRight="@drawable/icon_next"
            android:text="@string/women"
            android:textColor="@color/text_gray3"
            android:textSize="28px"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/bg_img_gray"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_20"
        android:background="@color/gray_lighter_f2"/>

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:id="@+id/city_layout"
        android:layout_width="match_parent"
        android:layout_height="88px"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/px_20"
        android:visibility="gone">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/city"
            android:textColor="@color/text_gray3"
            android:textSize="30px"/>

        <TextView
            android:id="@+id/tv_city"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="@dimen/px_20"
            android:drawableRight="@drawable/icon_next"
            android:textColor="@color/text_gray3"
            android:textSize="28px"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/bg_img_gray"/>

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:id="@+id/phone_bind_layout"
        android:layout_width="match_parent"
        android:layout_height="88px"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/px_20">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="绑定手机号"
            android:textColor="@color/text_gray3"
            android:textSize="30px"/>

        <TextView
            android:id="@+id/tv_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="@dimen/px_20"
            android:drawableRight="@drawable/icon_next"
            android:textColor="@color/text_gray3"
            android:textSize="28px"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/px_20"
        android:background="@color/bg_img_gray"/>

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:id="@+id/age_layout"
        android:layout_width="match_parent"
        android:layout_height="88px"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/px_20">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/age"
            android:textColor="@color/text_gray3"
            android:textSize="30px"/>

        <TextView
            android:id="@+id/tv_age"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="@dimen/px_20"
            android:drawableRight="@drawable/icon_next"
            android:textColor="@color/text_gray3"
            android:textSize="28px"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/px_20"
        android:background="@color/bg_img_gray"/>

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:id="@+id/star_sign_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/px_20">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/star_sign"
            android:textColor="@color/text_gray3"
            android:textSize="30px"/>

        <TextView
            android:id="@+id/tv_star"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="@dimen/px_20"
            android:drawableRight="@drawable/icon_next"
            android:textColor="@color/text_gray3"
            android:textSize="28px"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/px_20"
        android:background="@color/bg_img_gray"/>

    <com.bis.android.plug.autolayout.AutoLinearLayout
        android:id="@+id/professional_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/px_20">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/professional"
            android:textColor="@color/text_gray3"
            android:textSize="30px"/>

        <TextView
            android:id="@+id/tv_professional"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="@dimen/px_20"
            android:drawableRight="@drawable/icon_next"
            android:textColor="@color/text_gray3"
            android:textSize="28px"/>
    </com.bis.android.plug.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/gray_lighter_f2"/>
</com.bis.android.plug.autolayout.AutoLinearLayout>
