<?xml version="1.0" encoding="utf-8"?><!--空心圆角矩形  -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="oval"
       android:useLevel="false">


    <!-- 圆角半径 -->
    <solid android:color="@color/text_color_orange"/>
    <stroke
        android:width="1dp"
        android:color="@color/text_color_orange"/>
    <!--矩形的边线  -->

    <size android:width="20dp"
    android:height="20dp"/>

</shape>
